{
  "name": "ewuccc_client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "node server",
    "build": "npm run build:client && npm run build:server && npm run copy",
    "build:noExternal": "npm run build:client && npm run build:server:noExternal",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.js --outDir dist/server",
    "build:server:noExternal": "vite build --config vite.config.noexternal.js --ssr src/entry-server.js --outDir dist/server",
    "generate": "vite build --ssrManifest --outDir dist/static && npm run build:server && node prerender",
    "serve": "NODE_ENV=production node server",
    "debug": "node --inspect-brk server",
    "copy": "cp package.json server.js src/registerServiceWorker.js src/service-worker.js dist/client"
  },
  "dependencies": {
    "@aacassandra/vue3-progressbar": "^1.0.3",
    "@kyvg/vue3-notification": "^2.7.0",
    "@popperjs/core": "^2.11.6",
    "@tinymce/tinymce-vue": "^5.0.0",
    "@vueform/multiselect": "^2.5.7",
    "@vueup/vue-quill": "^1.0.0",
    "@vueuse/head": "^1.1.26",
    "axios": "^1.4.0",
    "bootstrap": "^5.2.3",
    "chart.js": "^4.3.0",
    "express": "^4.18.2",
    "html2canvas": "^1.4.1",
    "html2pdf.js": "^0.10.1",
    "js-cookie": "^3.0.1",
    "jspdf": "^2.5.1",
    "mitt": "^3.0.0",
    "moment": "^2.29.4",
    "slugify": "^1.6.6",
    "tinymce": "^6.3.1",
    "vform": "^2.1.2",
    "vue": "^3.2.41",
    "vue-chartjs": "^5.2.0",
    "vue-marquee-text-component": "^2.0.1",
    "vue-recaptcha": "^3.0.0-alpha.2",
    "vue-router": "^4.0.13",
    "vue-social-sharing": "^4.0.0-alpha4",
    "vue-sweetalert2": "^5.0.5",
    "vue3-carousel": "^0.3.1",
    "vue3-recaptcha2": "^1.7.0",
    "vuex": "^4.0.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.2.0",
    "vite": "^3.2.3",
    "vite-plugin-pwa": "^0.14.7"
  }
}
